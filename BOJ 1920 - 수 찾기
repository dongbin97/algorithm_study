#include <stdio.h>
#include <vector>
#include <algorithm>
using namespace std;

vector<int> src, input;
int N;

int binary_search( int target) {
	int left = 0, right = N - 1,mid;
	while (left <= right) {
		mid = (left + right) / 2;
		if (target == src[mid]) return 1;
		else if (target < src[mid])
			right = mid - 1;
		else left = mid + 1;
	}
	return 0;
}

int main() {
	int M, a;
	scanf("%d", &N);
	for (int i = 0; i < N; i++) {
		scanf("%d", &a);
		src.push_back(a);
	}

	sort(src.begin(), src.end());

	scanf("%d", &M);
	for (int i = 0; i < M; i++) {
		scanf("%d", &a);
		input.push_back(a);
	}

	for (int i = 0; i < M; i++)
		printf("%d\n", binary_search(input[i]));

	return 0;
}
