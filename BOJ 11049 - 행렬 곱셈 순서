#include <stdio.h>
#define MAX (1 << 31) - 1

int dp[500][500];
int dim[501];

int main() {
	int N;
	scanf("%d", &N);
	for (int i = 0; i < N; i++) 
		scanf("%d %d", &dim[i], &dim[i + 1]);
	
	for (int i = 0; i < N; i++)
		dp[i][i] = 0;

	int local, temp;
	for (int i = N - 2; i >= 0; i--) {
		for (int j = i + 1; j < N; j++) {
			local = MAX;
			for (int k = i; k < j; k++) {
				temp = dp[i][k] + dp[k + 1][j] + dim[i] * dim[k + 1] * dim[j + 1];
				if (local > temp)
					local = temp;
			}
			dp[i][j] = local;
		}
	}

	printf("%d", dp[0][N - 1]);
	return 0;
}
