#include <iostream>
#include <vector>
using namespace std;

int N, num[20][20], mins = 1000000;
void cal(vector<int>& first, vector<int>& second) {
	int i, j, sum1 = 0, sum2 = 0;
	int a = first.size(), b = second.size();
	for (i = 0; i < a; i++) {
		for (j = 0; j < a; j++) {
			sum1 += num[first[i]][first[j]];
		}
	}
	for (i = 0; i < b; i++) {
		for (j = 0; j < b; j++) {
			sum2 += num[second[i]][second[j]];
		}
	}
	int n = abs(sum1 - sum2);
	if (mins > n)
		mins = n;
}
int main() {
	int i, j;
	scanf("%d", &N);
	for (i = 0; i < N; i++)
		for (j = 0; j < N; j++)
			scanf("%d", &num[i][j]);
	for (i = 0; i < (1 << N); i++) {
		vector<int> first, second;
		for (j = 0; j < N; j++) {
			if (i & (1 << j))
				first.push_back(j);
			else
				second.push_back(j);
		}
		cal(first, second);
	}
	printf("%d\n", mins);
	return 0;
}
