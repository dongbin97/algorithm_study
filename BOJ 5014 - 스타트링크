#include <stdio.h>

using namespace std;

int main() {
	int F, S, G, U, D;
	scanf("%d %d %d %d %d", &F, &S, &G, &U, &D);
	for (int i = 0; S > 0 && S <= F && i <= F; i++) {
		if (S == G) {
			printf("%d\n", i);
			return 0;
		}
		if ((S > G && S - D > 0) || (S < G && S + U > F)) S -= D;
		else S += U;
	}
	printf("use the stairs\n");
	return 0;
}
