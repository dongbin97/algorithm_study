#include <iostream>
#include <algorithm>

using namespace std;
int a[10],num[12];
void perm(int i,int selected,int n) {
	int j;
	if (selected == 6) {
		for (j = 0; j < 6; j++) {
			printf("%d ", a[j]);
		}
		printf("\n");
		return;
	}
	if (i >= n) return;
	a[selected] = num[i];
	perm(i+1, selected + 1, n);
	a[selected] = 0;
	perm(i + 1, selected, n);
}
int main() {
	int N, i, j;
	while (1) {
		scanf("%d", &N);
		if (N == 0) return 0;
		for (i = 0; i < N; i++)
			scanf("%d", &num[i]);
		sort(num, num + N);
		perm(0,0, N);
		printf("\n");
	}
}
