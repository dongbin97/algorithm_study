#include <stdio.h>

int H, W, N;
int map[1001][1001];
int cnt[1001][1001] = { 0, };

int main() {
	scanf("%d %d %d", &H, &W, &N);
	for (int i = 0; i < H; i++)
		for (int j = 0; j < W; j++)
			scanf("%d", &map[i][j]);

	cnt[0][0] = N - 1;
	for (int i = 0; i < W; i++) {
		if (cnt[0][i] % 2 == 0)
			cnt[0][i + 1] = cnt[0][i] / 2;
		else {
			if (map[0][i] == 1)
				cnt[0][i + 1] = (cnt[0][i] + 1) / 2;
			else
				cnt[0][i + 1] = (cnt[0][i] - 1) / 2;
		}
	}

	for (int i = 0; i < H; i++) {
		if (cnt[i][0] % 2 == 0)
			cnt[i+1][0] = cnt[i][0] / 2;
		else {
			if (map[i][0] == 1)
				cnt[i+1][0] = (cnt[i][0] - 1) / 2;
			else
				cnt[i+1][0] = (cnt[i][0] + 1) / 2;
		}
	}
		
	for (int i = 1; i < H; i++) {
		for (int j = 1; j < W; j++) {
			if (cnt[i][j - 1] % 2 == 0)
				cnt[i][j] += cnt[i][j - 1] / 2;
			else {
				if (map[i][j - 1] == 1)
					cnt[i][j] += (cnt[i][j - 1] + 1) / 2;
				else
					cnt[i][j] += (cnt[i][j - 1] - 1) / 2;
			}
			if (cnt[i - 1][j] % 2 == 0)
				cnt[i][j] += cnt[i - 1][j] / 2;
			else {
				if (map[i - 1][j] == 1)
					cnt[i][j] += (cnt[i - 1][j] - 1) / 2;
				else
					cnt[i][j] += (cnt[i - 1][j] + 1) / 2;
			}
		}
	}

	for (int i = 0; i < H; i++) {
		for (int j = 0; j < W; j++) {
			if (cnt[i][j] % 2 == 1)
				map[i][j] = 1 - map[i][j];
		}
	}
	
	int i = 0, j = 0;
	while (i < H && j < W) {
		if (map[i][j] == 1) j++;
		else i++;
	}

	printf("%d %d", i + 1, j + 1);
	return 0;
}
