#include <stdio.h>

int N, num[100];
long long dp[100][21] = { 0, };
long long ans = 0;

int main() {
	scanf("%d", &N);
	for (int i = 0; i < N; i++)
		scanf("%d", &num[i]);

	
	dp[N - 2][num[N - 1]] = 1;
	for (int i = N - 3; i >= 0; i--) {
		for (int j = 0; j <= 20; j++) {
			if (dp[i + 1][j] > 0) {
				if (j - num[i + 1] >= 0) 
					dp[i][j - num[i + 1]] += dp[i + 1][j];
					
				if (j + num[i + 1] <= 20) 
					dp[i][j + num[i + 1]] += dp[i + 1][j];
			}
		}
	}

	printf("%lld", dp[0][num[0]]);
	return 0;
}
