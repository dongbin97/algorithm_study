#include <stdio.h>
#define MAX 1000000000
int dp[201][201];
char ans[200];

int main() {
	int N, M, K;
	scanf("%d %d %d", &N, &M, &K);

	for (int i = 0; i <= 200; i++) {
		dp[0][i] = 0;
		dp[i][0] = 1;
		dp[i][i] = 1;
	}

	for (int i = 1; i <= 200; i++)
		for (int j = 1; j < i; j++) {
			if (dp[i - 1][j] + dp[i - 1][j - 1] > MAX)
				dp[i][j] = MAX;
			else dp[i][j] = dp[i - 1][j] + dp[i - 1][j - 1];
		}

	if (K > dp[N + M][N]) {
		printf("-1");
		return 0;
	}

	int cnt_a = N, cnt_z = M;
	for (int i = 0; i < N + M; i++) {
		int temp = dp[cnt_a + cnt_z - 1][cnt_z];
		if (temp >= K) {
			ans[i] = 'a';
			cnt_a--;
		}
		else {
			ans[i] = 'z';
			cnt_z--;
			K -= temp;
		}
	}

	for (int i = 0; i < N + M; i++)
		printf("%c", ans[i]);
}
