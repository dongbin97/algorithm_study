#include <stdio.h>
#include <queue>
#include <cmath>
#include <algorithm>
using namespace std;

int K, N;
int num[100];
priority_queue<int, vector<int>, greater<int> > pq;

int main() {
	long long max = pow(2, 31);
	scanf("%d %d", &K, &N);

	for (int i = 0; i < K; i++) {
		scanf("%d", &num[i]);
		pq.push(num[i]);
	}

	sort(num, num + K);
	
	int cnt = 0, x;
	for(int i=0;i<N;i++){
		x = pq.top(); pq.pop();
		for (int j = 0; j < K; j++) {
			long long t = (long long) x * (long long) num[j];
			if (t >= max) continue;
			pq.push(t);
			if (x % num[j] == 0) break;
		}
	}
	
	printf("%d", x);
	return 0;
}
