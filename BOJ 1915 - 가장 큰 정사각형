#include <stdio.h>
#include <algorithm>
using namespace std;

int dp[1000][1000];

int main() {
	int N, M;
	scanf("%d %d", &N, &M);
	int ans = 0;

	for (int i = 0; i < N; i++)
		for (int j = 0; j < M; j++) {
			scanf("%1d", &dp[i][j]);
			if (dp[i][j] == 1) ans = 1;
		}

	for (int i = 1; i < N; i++) {
		for (int j = 1; j < M; j++) {
			if (dp[i][j] == 0) continue;
			else if (dp[i - 1][j] * dp[i - 1][j - 1] * dp[i][j - 1] == 0) continue;
			else {
				dp[i][j] = min(dp[i - 1][j], min(dp[i - 1][j - 1], dp[i][j - 1])) + 1;
				if (ans < dp[i][j]) ans = dp[i][j];
			}
		}
	}

	
	printf("%d", ans * ans);
	return 0;
}
