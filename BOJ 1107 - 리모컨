#include <stdio.h>

int num[10] = { 0, };

int check(int c) {
	int len = 0;
	while (1) {
		if (num[c % 10] == 1) return 0;
		else {
			len++;
			c /= 10;
			if (c == 0) break;
		}
	}
	return len;
}
int main() {
	int N, count, M, n, c, k;
	scanf("%d %d", &N, &M);
	for (int i = 0; i < M; i++) {
		scanf("%d", &n);
		num[n] = 1;
	}

	count = N > 100 ? N - 100 : 100 - N;
	for (int i = 0; i <= 1000000; i++) {
		k = check(i);
		if (k > 0) {
			c = i < N ? N - i : i - N;
			c += k;
			count = count > c ? c : count;
		}
	}
	printf("%d\n", count);
}
