#include <stdio.h>

int T;
long long dp[101];

int main() {
	int T, num;
	scanf("%d", &T);

	while (T--) {
		scanf("%d", &num);
		
		dp[0] = 0;
		for (int i = 1; i <= num; i++) {
			if (i == 1 || i == 2 || i == 3)
				dp[i] = 1;
			else if (i == 4)
				dp[i] = 2;
			else
				dp[i] = dp[i - 1] + dp[i - 5];
		}
		printf("%lld\n", dp[num]);
	}
}
