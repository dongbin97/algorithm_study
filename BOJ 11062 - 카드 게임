#include <stdio.h>
#include <algorithm>
using namespace std;

int T, N, card[1001],dp[1001][1001];

int main() {
	scanf("%d", &T);
	while (T--) {
		scanf("%d", &N);
		for (int i = 1; i <= N; i++)
			scanf("%d", &card[i]);

		for (int i = 1; i < N; i++) {
			dp[i][i] = card[i];
			dp[i][i + 1] = max(card[i], card[i + 1]);
		}
		dp[N][N] = card[N];

		for (int i = N - 2; i >= 1; i--) {
			for (int j = i + 2; j <= N; j++) {
				if (dp[i][j - 1] > dp[i + 1][j]) {
					if (dp[i + 1][j - 1] > dp[i + 2][j])
						dp[i][j] = card[i] + dp[i + 2][j];
					else
						dp[i][j] = card[i] + dp[i + 1][j - 1];
				}
				else {
					if (dp[i][j - 2] > dp[i + 1][j - 1])
						dp[i][j] = dp[i + 1][j - 1] + card[j];
					else
						dp[i][j] = dp[i][j - 2] + card[j];
				}
			}
		}

		printf("%d\n", dp[1][N]);
	}
	return 0;
}
