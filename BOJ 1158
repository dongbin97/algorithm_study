#include <stdio.h>
#include <queue>
using namespace std;

int main() {

	int N, K, i,a;
	queue<int> q;

	scanf("%d %d", &N, &K);
	
	for (i = 1; i <= N; i++)
		q.push(i);

	printf("<");
	while (!q.empty()) {
		if (q.size() == 1) {
			a = q.front(); q.pop();
			printf("%d>", a);
		}
		else {
			for (i=1; i < K; i++) {
			a = q.front(); q.pop(); q.push(a);
			}
			a = q.front(); q.pop();
			printf("%d, ", a);
		}
	}

	return 0;
}
