#include <iostream>
#include <vector>
using namespace std;

char c[21][21];
int N, M, ans = 0;
int dx[4] = { -1,0,1,0 }, dy[4] = { 0,1,0,-1 };
bool visited[26];

void dfs(int x, int y,int cnt) {
	int flag = 0;
	visited[c[x][y]-'A'] = true;
	for (int i = 0; i < 4; i++) {
		int nx = x + dx[i], ny = y + dy[i];
		if (0 <= nx && nx < N && 0 <= ny && ny < M) {
			if (visited[c[nx][ny]-'A'] == false) {
				flag = 1;
				dfs(nx, ny,cnt+1);
			}
		}
	}
	visited[c[x][y]-'A'] = false;
	if (flag == 0) {
		if (ans < cnt)
			ans = cnt;
	}
}

int main() {
	scanf("%d %d", &N, &M);
	for (int i = 0; i < N; i++)
		scanf("%s", c[i]);

	dfs(0, 0, 1);
	printf("%d\n", ans);
}
