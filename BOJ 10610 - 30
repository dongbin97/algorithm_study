#include <stdio.h>
#include <string.h>
#include <algorithm>
using namespace std;

bool comp(char a, char b) {
	return a > b;
}

int main() {
	char str[100000];
	bool flag = true;
	scanf("%s", &str);

	int sum = 0, len = strlen(str);
	for (int i = 0; i<len; i++) {
		sum = sum + (str[i] - '0');
	}

	if (sum % 3 != 0) flag = false;

	if (!flag) {
		printf("-1");
		return 0;
	}
	sort(str, str + len, comp);

	if (str[len - 1] != '0') flag = false;

	if (!flag) {
		printf("-1");
		return 0;
	}

	printf("%s", str);
	return 0;
}
