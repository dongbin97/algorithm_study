#include <stdio.h>

int main() {
	int N, M, arr[10000];
	scanf("%d %d", &N, &M);
	for (int i = 0; i < N; i++)
		scanf("%d", &arr[i]);

	int i = 0, j = 0, sum = arr[0], cnt = 0;
	while (j < N) {
		if (sum < M) {
			if (j + 1 < N) {
				sum += arr[j + 1];
				j++;
			}
			else break;
		}
		else if (sum > M) {
			sum -= arr[i++];
		}
		else {
			cnt++;
			if (j + 1 < N) {
				sum += arr[j + 1];
				j++;
			}
			else break;
		}
	}

	printf("%d", cnt);
	return 0;
}
