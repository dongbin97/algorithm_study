#include <stdio.h>

int H, W;
int dp[101][101][2] = { 0, };

int main() {
	scanf("%d %d", &W, &H);
	int max = H > W ? H : W;

	for (int i = 0; i <= max; i++) {
		dp[1][i][0] = 1; dp[1][i][1] = 1;
		dp[i][1][0] = 1; dp[i][1][1] = 1;
		dp[0][i][0] = 0; dp[0][i][1] = 0;
		dp[i][0][0] = 0; dp[i][0][1] = 0;
	}

	dp[1][1][0] = 0; dp[1][1][1] = 0;

	for (int i = 2; i <= H; i++) {
		for (int j = 2; j <= W; j++) {
			dp[i][j][0] = (dp[i - 1][j][0] + dp[i][j - 2][1]) % 100000;
			dp[i][j][1] = (dp[i][j - 1][1] + dp[i - 2][j][0]) % 100000;
		}
	}

	printf("%d", (dp[H - 1][W][0] + dp[H][W - 1][1]) % 100000);
	return 0;
}
