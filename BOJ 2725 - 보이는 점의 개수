#include <stdio.h>

int main() {
	int C, N;
	scanf("%d", &C);
	while (C--) {
		scanf("%d", &N);

		bool gcd[1001][1001] = { false, };

		int cnt = 1;
		for (int i = 1; i <= N; i++) {
			for (int j = i + 1; j <= N; j++) {
				if (gcd[i][j]) continue;
				for (int k = 2 * i, l = 2 * j; k <= N && l <= N; k = k + i, l = l + j)
					gcd[k][l] = true;
				cnt++;
			}
		}
		cnt = 2 * cnt + 1;
		printf("%d\n", cnt);
	}
	return 0;
}
