#include <iostream>
#include <vector>
#define SWAP(x,y,t) ((t)=(x),(x)=(y),(y)=(t))
using namespace std;

int main() {
	int N, i, j, n, max1 = 0, max2 ,temp;
	vector<int> num;
	scanf("%d", &N);
	for (i = 0; i < N; i++) {
		scanf("%d", &n);
		num.push_back(n);
	}
	for (i = 1; i < N; i++)
		if (num[i - 1] > num[i])
			max1 = i;
	if (max1 == 0) {
		printf("-1\n");
		return 0;
	}
	max2 = max1;
	for (j = max1; j < N; j++) 
		if (num[j] < num[max1-1])
			max2 = j;
	SWAP(num[max1-1], num[max2],temp);
	for (i = max1, j = N-1; i < j; i++, j--)
		SWAP(num[i], num[j], temp);
	for (i = 0; i < N; i++)
		printf("%d ", num[i]);
	printf("\n");		
}
