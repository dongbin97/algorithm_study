#include <stdio.h>
#include <string.h>
using namespace std;

long long GCD(long long a, long long b) {
	if (b == 0) return a;
	else return GCD(b, a % b);
}

int main() {
	int T;
	scanf("%d", &T);

	while (T--) {
		char str[20];
		
		scanf("%s", str);
		int len = strlen(str), left = -1, right = -1;
		for (int i = 2; i < len; i++) {
			if (str[i] == '(') left = i;
			else if (str[i] == ')') right = i;
		}

		if (left == -1 && right == -1) {
			long long num = 0, ten = 1;
			for (int i = 2; i < len; i++) {
				ten *= 10;
				num *= 10;
				num += (str[i] - '0');
			}
			long long g = GCD(num, ten);
			num = num / g; ten = ten / g;
			printf("%lld/%lld\n", num, ten);
		}
		else {
			long long num1 = 0, num2 = 0, ten1 = 1, ten2 = 1;
			bool find = false;
			for (int i = 2; i < len-1; i++) {
				if (str[i] == '(') {
					find = true;
					continue;
				}
				
				if (find == false) {
					ten1 *= 10;
					num1 *= 10;
					num1 += (str[i] - '0');
				}
				ten2 *= 10;
				num2 *= 10;
				num2 += (str[i] - '0');
			}
			long long ten = ten2 - ten1, num = num2 - num1;
			long long g = GCD(num, ten);
			num = num / g; ten = ten / g;
			printf("%lld/%lld\n", num, ten);
		}
	}
	return 0;
}
